				<text><![CDATA[ 4 CONSENTIMIENTO INFORMADO DEL PACIENTE]]></text>
			</staticText>
			<staticText>
				<reportElement x="0" y="15" width="415" height="46" uuid="c59391e1-80e8-4b36-b876-f2daa1ab1f0c"/>
				<box>
					<pen lineWidth="1.0"/>
